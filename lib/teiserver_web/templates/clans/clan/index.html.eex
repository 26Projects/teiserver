<% {_fg, _bg, bsname} = colours() %>

<%= render TeiserverWeb.Clans.GeneralView, "sub_menu.html", Map.merge(assigns, %{active: "clans"}) %>

<div class="row section-menu">
  <div class="col-md-12">
    <div class="card border-<%= bsname %>">
      <div class="card-body">

<%= for clan <- @clans do %>
  <div class="clan-box">
    <h4><%= clan.name %></h4>
    <h4>[<%= clan.tag %>]</h4>
  </div>
<% end %>

      </div>
    </div>
  </div>
</div>
